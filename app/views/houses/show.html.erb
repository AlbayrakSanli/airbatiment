    <div class="col-md-6">
      <div class="product-dtl">
        <div class="product-info">
          <div class="product-name"><%= @house.title %></div>
          <div class="reviews-counter">
            <div class="rate">
              <input type="radio" id="star5" name="rate" value="5" checked />
              <label for="star5" title="text">5 stars</label>
              <input type="radio" id="star4" name="rate" value="4" checked />
              <label for="star4" title="text">4 stars</label>
              <input type="radio" id="star3" name="rate" value="3" />
              <label for="star3" title="text">3 stars</label>
              <input type="radio" id="star2" name="rate" value="2" />
              <label for="star2" title="text">2 stars</label>
              <input type="radio" id="star1" name="rate" value="1" />
              <label for="star1" title="text">1 star</label>
            </div>
            <span>3 Reviews</span>
          </div>
          <div class="product-price-discount"><span><%= @house.price_visit %></span>€</div>
        </div>
        <div class="row">
          <div class="col-md-6">
            <p class="text-muted">
              Propriétaire : </br>
              <b><%= @owner.first_name %> <%= @owner.last_name %></b>
            </p>
          </div>
          <div class="col-md-6">
            <p>
              <span class="text-muted">Prix estimé du bien</span></br>
              <%= @house.price_house %> €
            </p>
          </div>

        </div>


        <div class="row">
          <div class="col-md-6">
            <p>
              <span class="text-muted">Surface</span></br>
              <%= @house.area %>m²
            </p>
          </div>
          <div class="col-md-6">
            <p>
              <span class="text-muted">Type</span></br>
              <%= @house.room %> pièces
            </p>
          </div>
        </div>

        <div class="row">
          <div class="col-md-6">
            <% if owner_signed_in? %>
              <% if @house.owner.id == current_owner.id %>
                <%= link_to 'Modifier infos', edit_house_path(@house), class:"btn btn-success" %>
                <%= link_to "Gérer les créneaux", new_availability_path(house_id: @house.id), class:"btn btn-warning" %>
                <%= link_to 'Supprimer', @house, method: :delete, data: { confirm: 'Est-ce que tu es sûr ?' }, class:"btn btn-danger" %>
              <% end %>
            <% end %>
          </div>
        </div>

        <% if user_signed_in? %>
          <div class="mb-3 col-md-12 border rounded"  style="max-height: 400px;">
            <div class="my-5">
              <h3><strong>Réservation</strong></h3>

              <%= form_tag house_path(@house), method: "get" do %>
                <div>
                  <div>
                    <%= label_tag 'Date du rendez-vous' %>
                    <%= date_field_tag 'date_start', Date.today, id: "date_select", class: 'form-control' %>
                    <br/>
                    <%= submit_tag "Choisir date", class: "mb-3 btn btn-outline-primary" %>
                  </div>
                </div>
               <% end %>

                <% @availabilities.each do |availability| %>
                 <% if availability.date_start.to_date == @date.to_date && availability.appointment.nil?%>
                  <%= availability.date_start.strftime("%d/%m/%Y à %H:%M") %> -
                  <% unless owner_signed_in? %>
                    <%= link_to "Prendre RDV", new_charge_path(id: availability.id, house_id: @house.id, user_id: current_user.id), class: "mt-1 btn-sm btn-primary" %>
                  <% end %>
                <% end %>
              <% end %>

            </div>
            <% end %>
        </div>

      </div>
    </div>
  </div>
